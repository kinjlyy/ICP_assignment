class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        int index=0;
        for(int i=0;i<n+m;i++)
        { if(i>m-1){
            nums1[i]=nums2[index];
            index++;
                
            }
        }
        sort(nums1);}
         public static int min_form_ith_index(int[] nums1, int i) {
		int mini=i;
		for(int j=i+1; j<nums1.length;j++) {
			if(nums1[j]<nums1[mini]) {
				mini=j;
			}
		}
		return mini;

	}
	public static void sort(int[] nums1) {
		for(int i=0; i<nums1.length;i++) {
			int idx=min_form_ith_index(nums1,i);
			int temp = nums1[i];
            nums1[i] = nums1[idx];
            nums1[idx] = temp;
			
		}
	}
}
